<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Syncfusion.Maui.Toolkit.PullToRefresh.SfPullToRefreshStyles"
             xmlns:pullToRefresh="clr-namespace:Syncfusion.Maui.Toolkit.PullToRefresh">

    <Style TargetType="pullToRefresh:SfPullToRefresh">
        <Setter Property="ProgressBackground"
                Value="{DynamicResource SfPullToRefreshProgressBackground}" />
        <Setter Property="ProgressColor"
                Value="{DynamicResource SfPullToRefreshProgressColor}" />
        <Setter Property="ProgressThickness"
                Value="{DynamicResource SfPullToRefreshProgressThickness}" />
    </Style>
</ResourceDictionary>